/include/ "system-conf.dtsi"
/ {
};

&gem0 {
    local-mac-address = [fa 61 0e 00 00 00];
    phy-handle = <&extphy0>;
    status = "okay";
    xlnx,has-mdio = <0x1>;
    psu_ethernet_0_mdio: mdio {
        #address-cells = <1>;
        #size-cells = <0>;
        extphy0: phy@1 {
            compatible = "realtek,RTL8211F";
            device_type = "ethernet-phy";
            reg = <1>;
        };
        gmii_to_rgmii_0: gmii_to_rgmii_0@8 {
            phy-handle = <&extphy0>;
            reg = <8>;
        };
    };
};  
      
&gem1 {
    local-mac-address = [fa 61 0e 00 01 00];
    phy-handle = <&extphy1>;
    status = "okay";
    xlnx,has-mdio = <0x1>;
    psu_ethernet_1_mdio: mdio {
        #address-cells = <1>;
        #size-cells = <0>;
        extphy1: phy@1 {
            compatible = "realtek,RTL8211F";
            device_type = "ethernet-phy";
            reg = <1>;
        };
        gmii_to_rgmii_1: gmii_to_rgmii_1@8 {
            phy-handle = <&extphy1>;
            reg = <8>;
        };
    };
};  
